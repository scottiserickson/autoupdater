<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context='.InstallApk'>


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/unknown_source"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:text="Unknown Source"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="101dp" />

        <Button
            android:id="@+id/my_source"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/unknown_source"
            android:layout_margin="16dp"
            android:text="my source"
            tools:layout_editor_absoluteX="123dp"
            tools:layout_editor_absoluteY="26dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:layout_marginBottom="359dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <Button
            android:id="@+id/uninstall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/my_source"
            android:layout_margin="16dp"
            android:text="uninstall"
            tools:layout_editor_absoluteX="142dp"
            tools:layout_editor_absoluteY="262dp" />

        <Button
            android:id="@+id/uninstall_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/uninstall"
            android:layout_margin="16dp"
            android:text="uninstall_result"
            tools:layout_editor_absoluteX="113dp"
            tools:layout_editor_absoluteY="188dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>